{
    "name": "solubis-ui",
    "version": "0.0.1",
    "registry": "github",
    "description": "Solubis UI",
    "repository": {
        "url": "https://github.com/solubis/ui"
    },
    "license": "Unlicense",
    "main": "js/index.js",
    "scripts": {
        "postinstall": "jspm install; jspm link",
        "style": "../bin/style-deps.sh"
